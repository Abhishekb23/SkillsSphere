{
  "version": 2,
  "builds": [
    {
      "src": "skillsphere-frontend/package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "skillsphere-frontend/dist/skillsphere-frontend/index.html"
    }
  ]
}
